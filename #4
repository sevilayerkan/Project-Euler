//project euler problem 4
#include <stdio.h>

int palindrom(int a);

int main ()
{
	int ans = 0, memory=0;
	
	for(int i=999; i>=100; i--)
	{
		for(int j=999; j>=100; j--)
		{
			ans = i*j;
			if(ans!=0 & ans>memory)
				if(palindrom(i*j)==1)
				{
					memory = ans;
				}
				
		}	
	}
	printf("%d",memory);
	
	return 0;
}

int palindrom(int x)
{
	
	int ters = 0;
	int sayi = x;
	while(x>0)
	{
		ters = ters*10 + x%10;
		x/=10;
	}
	
	if(sayi==ters)
		return 1;
	else
		return 0;
};
